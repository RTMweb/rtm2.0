{
  "functions": {
    "source": ".output/server",
    "predeploy": [
      "npm --prefix \"%RESOURCE_DIR\" run lint"
    ]
  },
  "hosting": {
    "site": "decent-envoy-295515",
    "public": ".output/public",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "server"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
